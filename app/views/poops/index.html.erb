<%= render "shared/header" %>

<p>poop management</p>

<% @posts.each do |post| %>
  <%= link_to post_path(post.id) do %>
  <div class = "post">
    <div class = "post-date">
      投稿日時：<%= post.created_at.strftime("%Y年 %m月 %d日 %H:%M") %>
    </div>
    <div class = "post-title">
      <%= post.title %>
    </div>
  </div>
  <% end %>
<% end %>