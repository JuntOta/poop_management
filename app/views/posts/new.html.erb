<%# CSS・・・assets/stylesheets/post/new.css %>

<%= render "shared/header" %>

<div class="main">
  <div class="inner">
    <div class="form__wrapper">
      <h2 class="page-heading">相談投稿</h2>
        <%# 部分テンプレートでフォームを表示する %>
        <%# <%= render partial: 'form', locals: { prototype: @prototype } %>
       <%= form_with local: true do |f|%> <%#モデル名記述する%>
        <div class="field">
          <%= f.label :title, "タイトル" %><br />
          <%= f.text_field :title, id:"prototype_title" %>
        </div>

        <div class="field">
            <%= f.label :concept, "内容" %><br />
           <%= f.text_area :concept, class: :form__text, id:"prototype_concept" %>
        </div>

        <div class="field">
            <%= f.label :image, "プロトタイプの画像" %><br />
           <%= f.file_field :image, id:"prototype_image" %>
        </div>

         <div class="actions">
           <%= f.submit "保存する", class: :form__btn  %>
        </div>
        <% end %>
    </div>
  </div>
</div>